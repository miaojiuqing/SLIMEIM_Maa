{
    "name": "Maa_SLIMEIM",
    "url": "https://github.com/miaojiuqing/SLIMEIM_Maa",
    "mirrorchyan_rid": "SLIMEIM_Maa",
    "mirrorchyan_multiplatform": false,
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        },
        {
            "name": "桌面端",
            "type": "Win32",
            "win32": {
                "class_regex": ".*",
                "window_regex": "Visual Studio"
            }
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource/"
            ]
        },
        {
            "name": "B 服",
            "path": [
                "{PROJECT_DIR}/resource/",
                "{PROJECT_DIR}/resource/bilibili"
            ]
        }
    ],
    "task": [
        {
            "name": "进入游戏并打开菜单",
            "entry": "进入游戏并打开菜单"
        },
        {
            "name": "日常-送上礼物",
            "entry": "每日任务之喂食角色",
            "doc": "固定点击第三列第三位角色,如果你满好感度角色有大于等于9位的话请联系作者增加功能"
        },
        {
            "name": "日常-城镇对话",
            "entry": "每日任务之好感度对话"
        },
        {
            "name": "日常-建筑支援请求",
            "entry": "日常任务之请求建筑支援"
        },
        {
            "name": "日常-锻造场升级装备",
            "entry": "锻造场升级任务"
        },
        {
            "name": "日常-捕食战",
            "entry": "每日任务之捕食战",
            "option": [
                "选择捕食类型"
            ],
            "doc": "可自由选择捕食类型,默认捕食类型为转魂"
        },
        {
            "name": "日常-领取日常任务奖励",
            "entry": "进入任务领取界面",
            "doc": "把这个任务添加三遍好像可以领取三个界面的任务）？"
        },
        {
            "name": "活动-并列演算20次",
            "entry": "版本活动限定-每日并列演算",
            "doc": "使用前请确保自己已解锁可以并列演算的关卡\n 请把自己最高收集特殊效果的编队放置于第七编队"
        },
        {
            "name": "活动-活动捕食战每天3次",
            "entry": "活动期间捕食战-总任务层",
            "doc": "自己过一下剧情"
        },
        {
            "name": "琐事-生产资源统一领取",
            "entry": "统一回收总任务"
        },
        {
            "name": "琐事-泡温泉领体力",
            "entry": "泡温泉领体力"
        },
        {
            "name": "琐事-进联盟签到领奖励",
            "entry": "每日点一下盟主联合"
        },
        {
            "name": "琐事-领取邮箱奖励",
            "entry": "邮箱领取"
        },
        {
            "name": "琐事-一并支援奖励领取",
            "entry": "一并支援奖励领取"
        },
        {
            "name": "活动-自动翻牌",
            "entry": "活动限定-自动翻牌总任务",
            "doc": "可能会会选择到一些不好的道具,虽然优先选择的是上面的道具但是因为超级优化可能会选到一些不好的道具,不会选地图,因为我没截到,下版本再加"
        },
        {
            "name": "自动英桀杯一般战战斗-公式化战斗逻辑",
            "entry": "点击进入英桀杯",
            "doc": "此任务仅支持公式化战斗逻辑,什么叫公式化战斗逻辑进群提问或看文档\n [size:15][color:tomato]请先自己提前调整为手动战斗状态自动切换战斗状态以后再说[/color][/size]\n 阴阳萌自己放在该放的地方,考虑到风露米有三转技能,请将主C放于5号位,三转工具人6号位"
        },
        {
            "name": "梦幻镜魔境-单层刷直到没体力",
            "entry": "梦幻镜魔境-任务总层",
            "doc": "此任务暂时只会在一个层里一直刷下去直到没体力,不考虑切换队伍和战斗失败的情况,有问题跟作者说作者改"
        }
    ],
    "option": {
        "选择捕食类型": {
            "cases": [
                {
                    "name": "捕食类型-转魂",
                    "pipeline_override": {
                        "选择捕食类型": {
                            "expected": [
                                "轉魂強化任務"
                            ]
                        }
                    }
                },
                {
                    "name": "捕食类型-心体",
                    "pipeline_override": {
                        "选择捕食类型": {
                            "expected": [
                                "心體強化任務"
                            ]
                        }
                    }
                },
                {
                    "name": "捕食类型-坚守",
                    "pipeline_override": {
                        "选择捕食类型": {
                            "expected": [
                                "堅守強化任務"
                            ]
                        }
                    }
                },
                {
                    "name": "捕食类型-猛攻",
                    "pipeline_override": {
                        "选择捕食类型": {
                            "expected": [
                                "猛攻強化任務"
                            ]
                        }
                    }
                }
            ],
            "default_case": "轉魂強化任務"
        }
    }
}